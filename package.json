{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "teletype",
  "title": "Teletype",
  "description": "A mininal version of the deta space teletype",
  "icon": "deta.png",
  "author": "SlumberDemon",
  "license": "MIT",
  "preferences": [
    {
      "name": "spaceToken",
      "type": "password",
      "title": "Space Token",
      "description": "Your deta space token",
      "required": true
    }
  ],
  "commands": [
    {
      "name": "search-discovery",
      "description": "Open the deta space discovery page",
      "title": "Search Discovery",
      "mode": "view"
    },
    {
      "name": "search-installed-apps",
      "title": "Search Installed Apps",
      "description": "Search for installed apps",
      "mode": "view"
    },
    {
      "name": "search-projects",
      "title": "Search your Projects",
      "description": "Search your deta space projects",
      "mode": "view"
    },
    {
      "name": "search-docs",
      "title": "Search Docs",
      "description": "Search the deta docs",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.48.4",
    "@raycast/utils": "^1.4.18",
    "deta-space-client": "^1.0.3"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
